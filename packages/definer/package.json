{
  "name": "@agaroot/eslint-config-definer",
  "version": "1.0.5",
  "description": "ESLint config for AGAROOT",
  "keywords": [
    "agaroot",
    "eslint",
    "config"
  ],
  "homepage": "https://github.com/agaroot-technologies/eslint-config/blob/main/packages/definer/README.md",
  "bugs": {
    "url": "https://github.com/agaroot-technologies/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agaroot-technologies/eslint-config.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "pkgroll",
    "lint:code": "eslint .",
    "lint:type": "tsc --noEmit"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.5",
    "@types/eslint": "8.56.12",
    "eslint": "8.57.1",
    "pkgroll": "2.5.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  }
}
